<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.outsystems.plugins.inappbrowser.osinappbrowserlib">

    <uses-permission android:name="android.permission.INTERNET" />

    <queries>
        <intent>
            <action android:name="android.intent.action.VIEW" />

            <category android:name="android.intent.category.BROWSABLE" />

            <data android:scheme="http" />
        </intent>
        <intent>
            <action android:name="android.intent.action.VIEW" />

            <category android:name="android.intent.category.BROWSABLE" />

            <data android:scheme="https" />
        </intent>
    </queries>

    <application
        android:allowBackup="false"
        android:usesCleartextTraffic="false">
        <activity
            android:name=".views.OSIABWebViewActivity"
            android:exported="false"
            android:label="@string/title_activity_osiabweb_view"
            android:theme="@style/Theme.InAppBrowserSampleApp" />
        <activity
            android:name=".views.OSIABWebViewActivityNew"
            android:exported="false"
            android:label="OSIABWebViewActivityNew"
            android:theme="@style/Theme.AppCompat" />
    </application>

</manifest>